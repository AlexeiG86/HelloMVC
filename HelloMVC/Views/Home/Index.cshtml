@model List<Students>

@{
    ViewData["Title"] = "Home Page";
    var message = "Alex is cool";

    var students = new List<Students>
    {
        new Students(){FirstName = "Bob"},
        new Students(){FirstName = "Cob"},
        new Students(){FirstName = "Dob"},
        new Students(){FirstName = "Qob"}
    };
}



<h1>
    Say some statements :@message


</h1>

@foreach (var student in students)
{
    <li>
        @student.FirstName
    </li>
}


